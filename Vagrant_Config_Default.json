{
  "box":"opscode-ubuntu-1204",
  "box_url":"https://opscode-vm-bento.s3.amazonaws.com/vagrant/opscode_ubuntu-12.04-i386_chef-11.4.4.box",
  "forwarded_port":[
    {
      "guest":8000,
      "host":8000
    }
  ],
  "synced_folder":[
    [
      "./workspace",
      "/home/vagrant/workspace",
      "dmode=777",
      "fmode=777"
    ],
    [
      "./home/.virtualenvs",
      "/home/vagrant/.virtualenvs"
    ],
    [
      "./home/.ssh",
      "/home/vagrant/.ssh",
      "dmode=775",
      "fmode=600"
    ]
  ],
  "chef_cookbooks":[
    "vendor-cookbooks",
    "main-cookbooks"
  ],
  "chef_role":"roles",
  "chef_bags_path":"data_bags",
  "chef_recipe":[
    "apt",
    "vim",
    "git",
    "teracy-dev"
  ],
  "forward_agent":true,
  "workspace":[
    "/vagrant/workspace/readonly",
    "/vagrant/workspace/teracy",
    "/vagrant/workspace/personal"
  ],
  "git":{
    "user":{
      "name":"Teracy Dev",
      "email":"teracy-dev@teracy.com"
    },
    "color":true,
    "commit":{
      "template":true
    },
    "diff":{
      "tool":"vimdiff"
    },
    "merge":{
      "tool":"vimdiff"
    },
    "difftool":{
      "prompt":false
    }
  },
  "nodejs":{
    "enabled":false,
    "npm":[
      "grunt-cli",
      "bower"
    ]
  },
  "python":{
    "enabled":true,
    "pip":{
      "global":{

      }
    }
  },
  "ruby":{
    "enabled":false
  },
  "php":{
    "enabled":false
  },
  "java":{
    "enabled":true,
    "version":"7",
    "flavor":"openjdk",
    "maven":{
      "enabled":true,
      "version":"3"
    }
  },
  "php":{
    "enabled":false,
    "version":"5.3.10"
  },
  "gettext":false
}
